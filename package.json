{
  "name": "imdone-core",
  "version": "1.26.2",
  "description": "imdone-core",
  "main": "index.js",
  "scripts": {
    "build": "doctoc --github --notitle README.md && npm test",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html mocha \"lib/**/__tests__/**/*.spec.js\" \"test/**/*spec.js\"",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "lint": "jshint ./lib",
    "mocha": "mocha",
    "release": "release-it",
    "release-plan": "release-it --dry-run",
    "test": "export NODE_ENV=test && npm run coverage",
    "test-debug": "mocha debug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imdone/imdone-core.git"
  },
  "keywords": [
    "imdone",
    "TODO",
    "FIXME",
    "markdown",
    "issues",
    "task-board",
    "kanban"
  ],
  "author": "Jesse Piascik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imdone/imdone-core/issues"
  },
  "homepage": "https://github.com/imdone/imdone-core",
  "jshintConfig": {
    "strict": true,
    "node": true,
    "globals": {
      "jQuery": true,
      "console": false,
      "module": true,
      "document": true
    }
  },
  "dependencies": {
    "@kwvanderlinde/markdown-it-wikilinks": "^1.0.2",
    "async": "~1.5.0",
    "checksum": "^0.1.1",
    "cheerio": "^1.0.0-rc.10",
    "chrono-node": ">=2.2.4",
    "debug": "^2.2.0",
    "download": "^8.0.0",
    "eol": "^0.9.1",
    "escape-string-regexp": "^1.0.5",
    "fast-sort": "^2.2.0",
    "function-regex": "^1.0.2",
    "gray-matter": "^4.0.2",
    "ignore": "^5.2.0",
    "imdone-api": "^0.0.13",
    "isbinaryfile": "^3.0.2",
    "js-yaml": "^3.14.1",
    "json-fns": "^1.0.0",
    "lodash.assign": "^4.2.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnumber": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.noop": "^3.0.1",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.reject": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "lodash.some": "^4.6.0",
    "lodash.template": "^4.5.0",
    "lodash.union": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.where": "^3.1.0",
    "lodash.without": "^4.4.0",
    "markdown-it": "^12.3.2",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-mark": "^3.0.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "monquery": "github:imdone/node-monquery#0.2.1",
    "readdirp": "^3.6.0",
    "remove-markdown": "^0.3.0",
    "rimraf": "^3.0.2",
    "sane": "^4.1.0",
    "sanitize-filename": "^1.6.3",
    "sift": "^13.4.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "colors": "^1.4.0",
    "doctoc": "^2.1.0",
    "expect.js": "~0.3.1",
    "jshint": "^2.9.4",
    "json-stringify-safe": "~5.0.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "release-it": "^14.12.4",
    "should": "13.1.3",
    "sinon": "^13.0.0",
    "sinon-chai": "^3.7.0",
    "vuepress": "^1.8.2",
    "wrench": "~1.5.8"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js",
      "cjs",
      "mjs"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": "75",
    "timeout": "2000",
    "spec": [
      "lib/**/__tests__/**/*.spec.js",
      "test/**/*spec.js"
    ],
    "watch-files": [
      "lib/**/__tests__/**/*.spec.js",
      "test/**/*spec.js"
    ]
  }
}
