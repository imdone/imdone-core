  "mocha": {
    "diff": true,
    "extension": [
      "js",
      "cjs",
      "mjs"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": "75",
    "timeout": "2000",
    "spec": [
      "lib/**/__tests__/**/*.spec.js",
      "test/**/*spec.js"
    ],
    "watch-files": [
      "lib/**/__tests__/**/*.spec.js",
      "test/**/*spec.js"
    ]
  }
